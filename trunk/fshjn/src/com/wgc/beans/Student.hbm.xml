<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wgc.beans.Student" table="stu_info">
        <id name="xh" type="java.lang.String">
            <column name="xh" length="15" />
            <generator class="assigned" />
        </id>
        <property name="xm" type="java.lang.String">
            <column name="xm" length="10" not-null="true" />
        </property>
        <property name="xb" type="java.lang.String">
            <column name="xb" length="2" not-null="true" />
        </property>
        <property name="bjm" type="java.lang.String">
            <column name="bjm" length="10" not-null="true" />
        </property>
        <property name="csny" type="java.sql.Timestamp">
            <column name="csny" length="16" not-null="true" />
        </property>
        <property name="lxfs" type="java.lang.String">
            <column name="lxfs" length="15" />
        </property>
    </class>
</hibernate-mapping>
